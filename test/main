    while true do
        local v94
        v93, v94 = v91(v92, v93)
        if v93 == nil then
            break
        end
        local v95 = (v93 - 1) * 45 + p90
        local v96 = math.sin(math.rad(v95)) * vu85.radius
        local v97 = math.cos(math.rad(v95)) * vu85.radius
        v94.From = p89 + Vector2.new(v96, v97)
        v94.To = p89 + Vector2.new(v96 * 1.5, v97 * 1.5)
        v94.Visible = vu85.enabled
    end
end
